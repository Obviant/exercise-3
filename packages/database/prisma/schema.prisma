generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Solicitation {
  id                    Int                     @id @default(autoincrement())
  solicitationId        Int                     @unique
  solicitationNumber    String?
  title                 String?
  agency                String?
  program               String?
  phase                 String?
  branch                String?
  year                  Int?
  releaseDate           DateTime?
  openDate              DateTime?
  closeDate             DateTime?
  status                String?
  agencyUrl             String?
  rawJson               Json
  createdAt             DateTime                @default(now())
  updatedAt             DateTime                @updatedAt
}
